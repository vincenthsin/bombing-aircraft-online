{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "API_BASE_URL": "https://your-backend-domain.vercel.app",
    "SOCKET_URL": "https://your-backend-domain.vercel.app"
  }
}